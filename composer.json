{
    "name": "risingsun/helper",
    "description": "Larvel Helper",
    "type": "library",
    "authors": [
        {
            "name": "gaoxu",
            "email": "gaoxu529@126.com"
        }
    ],
    "require": {
        "laravel/framework": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "php": "^7.2 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "orchestra/testbench": "^5.0 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "risingsun\\Helper\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "risingsun\\Helper\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "risingsun\\Helper\\HelperServiceProvider"
            ]
        }
    }
}
